#!/usr/bin/env bash

if [ ! -e .make-library ]; then
  exit 0
fi

mode_identifier=$1
library_id=`cat .make-library`
library_name=lib$library_id.a

if [ "$IHQ" ]; then
  echo $mode_identifier $library_name
else
  echo "ar rcs $IH/$library_name [objects]"
fi

objects=`find $IH -name "*.o"`
ar rcs $IH/$library_name $objects
